{
  "name": "@eka-care/pagify-sdk",
  "version": "1.2.0",
  "description": "A powerful JavaScript SDK for rendering HTML content as paginated PDFs using Paged.js and html2pdf.js",
  "main": "dist/pagify.js",
  "module": "dist/pagify.esm.js",
  "types": "dist/pagify.d.ts",
  "files": [
    "dist",
    "src",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "rollup -c",
    "build:dev": "rollup -c --watch",
    "dev": "npm run build:dev",
    "prepublishOnly": "npm run build",
    "test": "jest",
    "lint": "eslint src/**/*.js",
    "lint:fix": "eslint src/**/*.js --fix"
  },
  "keywords": [
    "pdf",
    "html-to-pdf",
    "pagination",
    "paged-media",
    "document-generation",
    "print",
    "pagedjs",
    "html2pdf",
    "browser",
    "client-side"
  ],
  "author": "EKA Care <dev@eka.care>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/eka-care/Pagify-sdk.git"
  },
  "bugs": {
    "url": "https://github.com/eka-care/Pagify-sdk/issues"
  },
  "homepage": "https://github.com/eka-care/Pagify-sdk#readme",
  "dependencies": {
    "html2pdf.js": "^0.10.1",
    "pagedjs": "^0.4.3"
  },
  "devDependencies": {
    "@babel/core": "^7.22.0",
    "@babel/preset-env": "^7.22.0",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^25.0.0",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^15.0.0",
    "@rollup/plugin-terser": "^0.4.4",
    "eslint": "^8.42.0",
    "jest": "^29.5.0",
    "rollup": "^3.25.0",
    "typescript": "^5.1.0"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "directories": {
    "test": "test"
  }
}
